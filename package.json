{
  "name": "@sayaww/nomimono",
  "private": false,
  "version": "0.0.66",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sillyleo/nomimono.git"
  },
  "author": "Leo Lin",
  "license": "ISC",
  "type": "module",
  "main": "./dist/nomimono.umd.cjs",
  "module": "./dist/nomimono.js",
  "types": "./dist/dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nomimono.js",
      "require": "./dist/nomimono.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/sillyleo/nomimono/issues"
  },
  "homepage": "https://github.com/sillyleo/nomimono#readme",
  "scripts": {
    "token-type": "json-to-ts-cli src/theme.json > src/tokenType.d.ts",
    "dev": "json-to-ts-cli src/theme.json > src/tokenType.d.ts && SHOW_ALL_COMPONENT_DEMOS=true vite serve docs",
    "build-docs": "rimraf docs/dist && vite build docs",
    "ssr-docs": "rimraf docs/dist && vite-pages ssr docs",
    "build-lib": "vite build",
    "ship": "vite build && pnpm version patch && pnpm publish --access public"
  },
  "dependencies": {
    "@fontsource/inter-tight": "^4.5.2",
    "@macaron-css/core": "^1.0.0",
    "@macaron-css/react": "^1.0.1",
    "@stitches/react": "^1.2.8",
    "class-variance-authority": "^0.4.0",
    "framer-motion": "^8.5.0",
    "react": "^18.2.0",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@macaron-css/vite": "^1.1.3",
    "@mdx-js/react": "^2.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vanilla-extract/css": "^1.9.3",
    "@vitejs/plugin-react": "^3.0.1",
    "json-to-ts-cli": "^1.0.2",
    "lodash": "^4.17.21",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "serve": "^14.1.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-pages-theme-doc": "^4.0.1",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vite-plugin-react-pages": "^4.0.2"
  }
}
